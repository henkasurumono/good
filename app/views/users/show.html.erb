<div class="contents row" >
    <p><%= @nickname %>さんの投稿一覧</p>
    <% @articles.each do |article| %>
      <div class="content_post" style="background-image: url(<%= article.image %>);">
        <%= simple_format(article.title) %>
        <span class="name"><%= article.content %></span>
      </div>
    <% end %>
    <%= paginate(@articles) %>
  </div>