<div class="contents row">
  
  <%= form_tag('/articles', method: :post) do %>
    <h3>
      投稿する
    </h3>
    <p class=www><input type="text" name="title" placeholder="タイトル"></p>
    <p class=www><input placeholder="Image Url" type="text" name="image"></p>
    <p class=www><textarea name="content" placeholder="記事を書いてください" rows="10" cols="50"></textarea></p>
    <p id=bota><input type="submit" value="投稿"></p>
  <% end %>
  <a id=re href="/">一覧にもどる</a>
</div>